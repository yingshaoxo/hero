import "fmt"

string get_username() {
    print("What's your name, dear? ___\n")

    username := ""
    fmt.Scanln(&username)

    return username;
}

export void say_hi() {
    name := get_username()
    text := "\n\nHi!" + name + "!\n"
    print(text)
}

export void print_something() {
    print("Hello World\n\n")
}






